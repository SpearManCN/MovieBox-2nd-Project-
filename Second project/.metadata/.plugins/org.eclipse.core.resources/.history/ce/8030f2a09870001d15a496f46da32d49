package com.naver.erp;

public class ToolDTO {

		int toolNo;
	    int jijumNo;
	    String selectedDate;
	    String toolName;
	    int selJijumNo;
	    int selBigSort;
	    int selSmallSort;
	    String selKeyword;
	    
	    
	    
	    
	    
	    
	    
	    public int getSelJijumNo() {
			return selJijumNo;
		}
		public void setSelJijumNo(int selJijumNo) {
			this.selJijumNo = selJijumNo;
		}
		public int getSelBigSort() {
			return selBigSort;
		}
		public void setSelBigSort(int selBigSort) {
			this.selBigSort = selBigSort;
		}
		public int getSelSmallSort() {
			return selSmallSort;
		}
		public void setSelSmallSort(int selSmallSort) {
			this.selSmallSort = selSmallSort;
		}
		public String getSelKeyword() {
			return selKeyword;
		}
		public void setSelKeyword(String selKeyword) {
			this.selKeyword = selKeyword;
		}
		public int getToolNo() {
		return toolNo;
	}
	public void setToolNo(int toolNo) {
		this.toolNo = toolNo;
	}
	public int getJijumNo() {
		return jijumNo;
	}
	public void setJijumNo(int jijumNo) {
		this.jijumNo = jijumNo;
	}
	public String getSelectedDate() {
		return selectedDate;
	}
	public void setSelectedDate(String selectedDate) {
		this.selectedDate = selectedDate;
	}
	public String getToolName() {
		return toolName;
	}
	public void setToolName(String toolName) {
		this.toolName = toolName;
	}
		
	
	
	
}
